{
  "name": "browserify-vscode-work-space",
  "version": "1.0.0",
  "description": "Working with browserify & VSCode",
  "main": "app.js",
  "config": {
  },
  "scripts": {
    "build": "node ./bin/",
    "start": "browserify -t babelify -e ./src/app.js -o ./src/app/js/bundle.js",
